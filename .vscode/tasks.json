{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build KeyKeepers Client",
            "type": "shell",
            "command": "dotnet",
            "args": [
                "build",
                "${workspaceFolder}/KeyKeepersClient/KeyKeepersClient.csproj"
            ],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "silent",
                "focus": false,
                "panel": "shared"
            },
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Run KeyKeepers Client",
            "type": "shell",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/KeyKeepersClient/KeyKeepersClient.csproj"
            ],
            "group": "build",
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": true,
                "panel": "new"
            },
            "dependsOn": "Build KeyKeepers Client",
            "problemMatcher": "$msCompile",
            "isBackground": false
        }
    ]
}